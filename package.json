{
  "name": "bootstrap.native",
  "version": "4.1.4",
  "description": "Native JavaScript for Bootstrap, the sweetest JavaScript library without jQuery.",
  "main": "dist/bootstrap-native.min.js",
  "types": "src/index.d.ts",
  "module": "dist/bootstrap-native.esm.js",
  "jsnext": "src/index.js",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "help": "rollup --help",
    "build": "npm run lint:js && npm-run-all --parallel build-* && npm-run-all --parallel copy-*",
    "custom": "rollup -c --environment",
    "fix:js": "eslint src/ --config .eslintrc --fix",
    "lint:js": "eslint src/ --config .eslintrc",
    "build-esm": "rollup --environment INPUTFILE:./src/index.js,OUTPUTFILE:./dist/bootstrap-native-esm.js,FORMAT:esm,MIN:false -c",
    "build-esm-min": "rollup --environment INPUTFILE:./src/index.js,OUTPUTFILE:./dist/bootstrap-native-esm.min.js,FORMAT:esm,MIN:true -c",
    "build-umd": "rollup --environment INPUTFILE:./src/index.js,OUTPUTFILE:./dist/bootstrap-native.js,FORMAT:umd,MIN:false -c",
    "build-umd-min": "rollup --environment INPUTFILE:./src/index.js,OUTPUTFILE:./dist/bootstrap-native.min.js,FORMAT:umd,MIN:true -c",
    "copy-umd": "rollup --environment INPUTFILE:./src/index.js,OUTPUTFILE:./docs/assets/js/bootstrap-native.js,FORMAT:umd,MIN:false -c",
    "polyfill": "npm-run-all --parallel polyfill-*",
    "polyfill-unminified": "rollup --environment MIN:false -c rollup.polyfill.js",
    "polyfill-minified": "rollup --environment MIN:true -c rollup.polyfill.js",
    "polyfill-demo": "rollup --environment MIN:false,OUTPUTFILE:./docs/assets/js/polyfill.js -c rollup.polyfill.js",
    "watch": "rollup --environment FORMAT:umd,MIN:false -c -w"
  },
  "browserslist": [
    "last 2 versions",
    "not <= 1%",
    "not IE 10"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thednp/bootstrap.native.git"
  },
  "keywords": [
    "bootstrap.native",
    "bootstrap",
    "vanilla js",
    "native javascript",
    "vanilla javascript"
  ],
  "author": "dnp_theme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thednp/bootstrap.native/issues"
  },
  "homepage": "https://thednp.github.io/bootstrap.native/",
  "dependencies": {
    "minifill": "^0.0.17",
    "@thednp/event-listener": "^1.0.4",
    "@thednp/shorty": "^1.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^5.3.1"
  }
}
