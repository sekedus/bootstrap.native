{
  "name": "@thednp/bootstrap.native",
  "version": "5.0.0-alpha1",
  "description": "Native JavaScript for Bootstrap",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./dist/bootstrap.native.js",
  "types": "./dist/bootstrap.native.d.ts",
  "module": "./dist/bootstrap.native.mjs",
  "exports": {
    ".": {
      "import": "./dist/bootstrap-native.mjs",
      "require": "./dist/bootstrap-native.cjs",
      "types": "./dist/bootstrap-native.d.ts"
    }
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "pre-test": "npm run clean-coverage",
    "clean-coverage": "rimraf coverage .nyc_output",
    "test": "npm run pre-test && npx cypress run",
    "cypress": "npm run pre-test && npx cypress open",
    "coverage:report": "nyc report --reporter=lcov --reporter=json --reporter=text --reporter=json-summary",
    "build": "npm run lint:ts && npm run build-vite && npm run build-dts && npm run copy-docs",
    "build-vite": "vite build && npm run copy-docs",
    "build-dts": "dts-bundle-generator --config ./dts.config.ts",
    "copy-docs": "ncp dist/bootstrap-native.js docs/assets/js/bootstrap-native.js && ncp dist/bootstrap-native.js.map docs/assets/js/bootstrap-native.js.map",
    "lint:ts": "eslint -c .eslintrc.cjs --ext .ts src",
    "fix:ts": "eslint -c .eslintrc.cjs --ext .ts src --fix",
    "lint:js": "eslint src/ --config .eslintrc",
    "modules": "vite build --config ./vite.modules.ts",
    "watch": "rollup --environment FORMAT:umd,MIN:false -c -w"
  },
  "browserslist": [
    "last 2 versions",
    "not <= 1%",
    "not IE 10"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thednp/bootstrap.native.git"
  },
  "keywords": [
    "bootstrap lite",
    "bootstrap",
    "native",
    "javascript",
    "vanilla"
  ],
  "author": "dnp_theme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thednp/bootstrap.native/issues"
  },
  "homepage": "https://thednp.github.io/bootstrap.native/",
  "dependencies": {
    "@thednp/event-listener": "^2.0.0-alpha6",
    "@thednp/shorty": "^2.0.0-alpha7"
  },
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@cypress/code-coverage": "^3.10.0",
    "@types/istanbul-lib-instrument": "^1.7.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "cypress": "^12.0.2",
    "dts-bundle-generator": "^7.1.0",
    "eslint": "^8.29.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "istanbul-lib-coverage": "^3.2.0",
    "istanbul-lib-instrument": "^5.2.0",
    "ncp": "^2.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
